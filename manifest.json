{
  "manifest_version": 2,
  "name": "Vinted Brand Filter",
  "version": "1.0.0",
  "description": "Filter out unwanted brands from Vinted search results",
  "permissions": [
    "storage",
    "*://*.vinted.com/*",
    "*://*.vinted.fr/*",
    "*://*.vinted.be/*",
    "*://*.vinted.de/*",
    "*://*.vinted.nl/*",
    "*://*.vinted.es/*",
    "*://*.vinted.it/*",
    "*://*.vinted.pl/*",
    "*://*.vinted.lt/*",
    "*://*.vinted.cz/*",
    "*://*.vinted.sk/*",
    "*://*.vinted.at/*",
    "*://*.vinted.pt/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.vinted.com/*",
        "*://*.vinted.fr/*",
        "*://*.vinted.be/*",
        "*://*.vinted.de/*",
        "*://*.vinted.nl/*",
        "*://*.vinted.es/*",
        "*://*.vinted.it/*",
        "*://*.vinted.pl/*",
        "*://*.vinted.lt/*",
        "*://*.vinted.cz/*",
        "*://*.vinted.sk/*",
        "*://*.vinted.at/*",
        "*://*.vinted.pt/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
}